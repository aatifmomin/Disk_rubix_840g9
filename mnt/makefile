print_log = @if [ "$(5)" == 1 ]; then \
            echo "+--------------------------------------------------------------------------------+"; \
            fi; \
            echo "|make|$(1)|$(2)| $(3)"; \
            if [ "$(6)" == 1]; then \
                echo "+--------------------------------------------------------------------------------+"; \
            fi; \
            if [ "$(4)" == 1 ]; then echo ""; fi;\

print_log_n = if [ "$(5)" == 1]; then \
            echo "+--------------------------------------------------------------------------------+"; \
            fi; \
            echo "|make|$(1)|$(2)| $(3)"; \
            if [ "$(6)" == 1 ]; then \
                echo "+--------------------------------------------------------------------------------+"; \
            fi; \
            if [ "$(4)" == 1 ]; then echo ""; fi;\

#--------------------------------------------------------------------------------
#User Definded Variables
#--------------------------------------------------------------------------------


#--------------------------------------------------------------------------------
#System Generated Variables
#--------------------------------------------------------------------------------


#--------------------------------------------------------------------------------
#Functions
#--------------------------------------------------------------------------------

print_readme:
	@python3 1_flow_setup/python/print_readme.py;


help:
	@echo "+--------------------------------------------------------------------------------+"
	@echo "Global Help Command"
	@echo "+--------------------------------------------------------------------------------+"
	@echo "  1.make print_readme			- This will help to update readme"
	@echo "+--------------------------------------------------------------------------------+"
	@echo "Enter command number: "; \
	read number; \
	if [ $$number -eq 1 ]; then \
		make print_readme; \
	fi;
